# Database
# Full async URL used by the app and Alembic
DATABASE_URL=mysql+aiomysql://planner:password@localhost:3306/goal_agent

# Individual DB vars used by scripts/backup.sh and scripts/db_init.sh.
# Must match the credentials embedded in DATABASE_URL above.
DB_HOST=localhost
DB_PORT=3306
DB_NAME=goal_agent
DB_USER=planner
DB_PASSWORD=password

# Kimi AI (Moonshot, OpenAI-compatible)
KIMI_API_KEY=sk-your-key-here
KIMI_BASE_URL=https://api.moonshot.cn/v1
KIMI_MODEL_SHORT=moonshot-v1-8k
KIMI_MODEL_LONG=moonshot-v1-32k

# HMAC request signing (shared secret between OpenClaw plugin and API)
HMAC_SECRET=change-me-to-a-random-secret

# Telegram Bots
TELEGRAM_BEST_PAL_BOT_TOKEN=123456:ABCdef...
TELEGRAM_GO_GETTER_BOT_TOKEN=789012:GHIjkl...
TELEGRAM_GROUP_CHAT_ID=-1001234567890

# GitHub (for auto-committing plans and reports)
GITHUB_PAT=ghp_your_token_here
GITHUB_DATA_REPO=username/study-data-private
GITHUB_COMMITTER_NAME=Goal Agent Bot
GITHUB_COMMITTER_EMAIL=bot@example.com

# App
APP_SECRET_KEY=change-me-in-production
APP_DEBUG=false
APP_HOST=0.0.0.0
APP_PORT=8000

# Admin Telegram Chat IDs (comma-separated, bootstraps first admin)
ADMIN_CHAT_IDS=123456789
